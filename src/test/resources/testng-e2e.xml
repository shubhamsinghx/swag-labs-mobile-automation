<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SwagLabs-E2E-Suite" verbose="2" parallel="false">

    <parameter name="orientation" value="PORTRAIT"/>

    <listeners>
        <listener class-name="com.swaglabs.listeners.TestListener"/>
    </listeners>

    <test name="E2E Flow Test">
        <classes>
            <class name="com.swaglabs.tests.E2EFlowTest"/>
        </classes>
    </test>

</suite>
